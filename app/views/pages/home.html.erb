<%= form_with url: skills_path, method: :get, class: "search-box-home" do %>
  <%= text_field_tag :query,
    params[:query],
    class: "search-input",
    placeholder: "Find a skill"
  %>
  <button class="search-btn">
    <i class="fas fa-search"></i>
  </button>
<% end %>

